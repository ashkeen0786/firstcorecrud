@{
    ViewData["Title"] = "Home Page";
}

@model List<firstcorecrud.db.Coretablee>

<a asp-action="privacy" class="btn btn-primary">Add</a><br />


<table class="table table-hover">
    <tr>
        <th>id</th>
        <th>Name</th>
        <th>Email</th>
        <th>Phone</th>
        <th>Action</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>

            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Email</td>
            <td>@item.Phone</td>
            <td>
                <a href="@Url.Action("delete",new {item.Id })" class="btn btn-danger">Delete</a>
                <a href="@Url.Action("edit",new {item.Id })" class="btn btn-warning">Edit</a>
            </td>
        </tr>


    }


</table>
